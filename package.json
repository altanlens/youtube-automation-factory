{
  "name": "youtube-automation-station",
  "version": "1.0.0",
  "description": "Comprehensive YouTube video automation system",
  "main": "src/index.ts",
  "scripts": {
    "start": "remotion preview",
    "build": "remotion render",
    "upgrade": "remotion upgrade",
    "test": "jest",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "generate": "node scripts/generate-video.js",
    "upload": "node scripts/upload-to-youtube.js",
    "pipeline": "node scripts/run-pipeline.js",
    "create": "node scripts/create-ai-video.js",
    "create-from-prompt": "node scripts/create-video-from-prompt.js",
    "typecheck": "tsc --noEmit",
    "dev": "npm start",
    "render:fast": "node scripts/fast-render.js",
    "render:preview": "node scripts/fast-render.js SimpleExcalidraw ./output/preview.mp4 preview",
    "render:batch": "node scripts/batch-render.js",
    "render:batch-preview": "node scripts/batch-render.js test preview",
    "render:batch-fast": "node scripts/batch-render.js test production",
    "render:ultra-fast": "node scripts/batch-render.js test ultra_fast"
  },
  "keywords": [
    "youtube",
    "automation",
    "video",
    "remotion",
    "content"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@remotion/cli": "^4.0.0",
    "@remotion/eslint-config": "^4.0.0",
    "@remotion/paths": "^4.0.0",
    "@remotion/zod-types": "^4.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "google-tts-api": "^2.0.2",
    "googleapis": "^126.0.1",
    "pexels": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remotion": "^4.0.0",
    "winston": "^3.11.0",
    "zod": "3.22.3",
    "roughjs": "^4.6.6",
    "ajv": "^8.12.0",
    "uuid": "^9.0.1",
    "@excalidraw/excalidraw": "^0.17.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
